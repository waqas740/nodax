<div class="row">
    <div class="col-12">
        <h3> Create <%- moduleName %></h3>
    </div>

</div>
<hr>
<form method="post">
    <% Object.keys(cols).forEach(col => { %>
    <div class="form-group shadow-sm">
        <label><%- cols[col].toFLUpperCase() %> </label>
        <input type="text" name=<%- cols[col] %> class="form-control"
            placeholder="Enter <%- cols[col].toFLUpperCase() %>">
    </div>
    <%})%>

    <div class="text-right">
        <button type="submit" class="btn btn-primary shadow-sm">Submit</button>
    </div>

</form>