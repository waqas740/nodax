<div class="row">
    <div class="col-12">
        <h3> Edit <%- moduleName %></h3>
    </div>

</div>
<hr>
<form method="put" action="/<%- moduleName.toLowerCase()%>/<%%- data.id %>">
    <% Object.keys(cols).forEach(col => { %>
    <div class="form-group shadow-sm">
        <label><%- cols[col].toFLUpperCase() %> </label>
        <input type="text" name="<%- col %>" class=" form-control" placeholder="Enter <%- cols[col].toFLUpperCase() %>"
            value="<%%-data['<%-col%>']%>">
    </div>
    <%})%>

    <div class="text-right">
        <button type="submit" class="btn btn-primary shadow-sm">Update</button>
    </div>

</form>